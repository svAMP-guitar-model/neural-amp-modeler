{
    "_notes": [
        "This is the Wavenet NAM model that can handle a text embedding condition vector"
    ],
    "net": {
        "name": "PromptWaveNet",
        "config": {
            "layers_configs": [
                {
                    "condition_size": 128,
                    "input_size": 1,
                    "channels": 16,
                    "head_size": 8,
                    "kernel_size": 8,
                    "dilations": [
                        1,
                        2,
                        4,
                        8,
                        16,
                        32,
                        64,
                        128,
                        256,
                        512
                    ],
                    "activation": "Tanh",
                    "gated": false,
                    "head_bias": false
                },
                {
                    "condition_size": 128,
                    "input_size": 16,
                    "channels": 8,
                    "head_size": 1,
                    "kernel_size": 8,
                    "dilations": [
                        1,
                        2,
                        4,
                        8,
                        16,
                        32,
                        64,
                        128,
                        256,
                        512
                    ],
                    "activation": "Tanh",
                    "gated": false,
                    "head_bias": true
                }
            ],
            "head_scale": 0.02,
            "embedding_size": 768,
            "condition_size": 128
        }
    },
    "optimizer": {
        "lr": 0.004
    },
    "lr_scheduler": {
        "class": "ExponentialLR",
        "kwargs": {
            "gamma": 0.993
        }
    }
}